	<banner:BannerControl LinkToUnoPlatformApp="https://github.com/unoplatform/cybertruck"
						  AppName="Cybertruck"
						  AppAuthor="Uno Platform">
		<VisualStateManager.VisualStateGroups>
			<VisualStateGroup x:Name="DefaultState">
				<VisualState>
					<VisualState.StateTriggers>
						<StateTrigger IsActive="True" />
					</VisualState.StateTriggers>
					<Storyboard>
						<DoubleAnimation From="0"
										 To="-360"
										 Duration="0:0:0.5"
										 RepeatBehavior="Forever"
										 Storyboard.TargetName="TeslaLeftWheel"
										 Storyboard.TargetProperty="(UIElement.RenderTransform).(RotateTransform.Angle)" />
						<DoubleAnimation From="0"
										 To="-360"
										 Duration="0:0:0.5"
										 RepeatBehavior="Forever"
										 Storyboard.TargetName="TeslaRightWheel"
										 Storyboard.TargetProperty="(UIElement.RenderTransform).(RotateTransform.Angle)" />
						<DoubleAnimation From="0.5"
										 To="1"
										 Duration="0:0:0.08"
										 RepeatBehavior="Forever"
										 Storyboard.TargetName="FrontLight"
										 Storyboard.TargetProperty="Opacity" />
						<DoubleAnimation From="0.5"
										 To="1"
										 Duration="0:0:0.6"
										 RepeatBehavior="Forever"
										 Storyboard.TargetName="BackLight"
										 Storyboard.TargetProperty="Opacity" />
					</Storyboard>
				</VisualState>
			</VisualStateGroup>
			<VisualStateGroup x:Name="FirstGlassState">
				<VisualState>
					<VisualState.StateTriggers>
						<StateTrigger IsActive="{x:Bind FirstGlassBroken, Mode=TwoWay}" />
					</VisualState.StateTriggers>
					<Storyboard>
						<DoubleAnimation From="0"
										 To="0.8"
										 Duration="0:0:.250"
										 Storyboard.TargetName="FirstBrokenGlass"
										 Storyboard.TargetProperty="Opacity" />
					</Storyboard>
				</VisualState>
			</VisualStateGroup>
			<VisualStateGroup x:Name="SecondGlassState">
				<VisualState>
					<VisualState.StateTriggers>
						<StateTrigger IsActive="{x:Bind SecondGlassBroken, Mode=TwoWay}" />
					</VisualState.StateTriggers>
					<Storyboard>
						<DoubleAnimation From="0"
										 To="0.9"
										 Duration="0:0:.250"
										 Storyboard.TargetName="SecondBrokenGlass"
										 Storyboard.TargetProperty="Opacity" />
					</Storyboard>
				</VisualState>
			</VisualStateGroup>
		</VisualStateManager.VisualStateGroups>

