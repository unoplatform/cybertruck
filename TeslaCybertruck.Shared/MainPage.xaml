<Page
	x:Class="TeslaCybertruck.MainPage"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:local="using:TeslaCybertruck"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d">

	<Page.Resources>
		<Storyboard x:Name="GroundAnimation">
			<DoubleAnimation
				RepeatBehavior="Forever"
				Storyboard.TargetName="GroundMoving"
				Storyboard.TargetProperty="X"
				To="1000"
				Duration="0:0:3" />
		</Storyboard>

		<Storyboard x:Key="SpinTeslaLeftWheel">
			<DoubleAnimation
				RepeatBehavior="Forever"
				Storyboard.TargetName="TeslaLeftWheel"
				Storyboard.TargetProperty="(UIElement.RenderTransform).(RotateTransform.Angle)"
				To="-360"
				Duration="0:0:0.5" />
		</Storyboard>

		<Storyboard x:Key="SpinTeslaRightWheel">
			<DoubleAnimation
				RepeatBehavior="Forever"
				Storyboard.TargetName="TeslaRightWheel"
				Storyboard.TargetProperty="(UIElement.RenderTransform).(RotateTransform.Angle)"
				To="-360"
				Duration="0:0:0.5" />
		</Storyboard>

		<Storyboard x:Key="FrontLightOpacity">
			<DoubleAnimation
				RepeatBehavior="Forever"
				Storyboard.TargetName="FrontLight"
				Storyboard.TargetProperty="Opacity"
				To="0.5"
				Duration="0:0:0.8" />
		</Storyboard>

		<Storyboard x:Key="RotateFrontLight">
			<DoubleAnimation
				RepeatBehavior="Forever"
				Storyboard.TargetName="FrontLight"
				Storyboard.TargetProperty="(UIElement.RenderTransform).(RotateTransform.Angle)"
				To="1">
				<DoubleAnimation.EasingFunction>
					<BounceEase
						Bounces="10"
						Bounciness="2"
						EasingMode="EaseInOut" />
				</DoubleAnimation.EasingFunction>
			</DoubleAnimation>
		</Storyboard>

		<Storyboard x:Key="BackLightOpacity">
			<DoubleAnimation
				RepeatBehavior="Forever"
				Storyboard.TargetName="BackLight"
				Storyboard.TargetProperty="Opacity"
				To="0.5"
				Duration="0:0:0.5" />
		</Storyboard>

		<Storyboard x:Key="RotateBackLight">
			<DoubleAnimation
				RepeatBehavior="Forever"
				Storyboard.TargetName="BackLight"
				Storyboard.TargetProperty="(UIElement.RenderTransform).(RotateTransform.Angle)"
				To="0.1">
				<DoubleAnimation.EasingFunction>
					<BounceEase
						Bounces="10"
						Bounciness="2"
						EasingMode="EaseInOut" />
				</DoubleAnimation.EasingFunction>
			</DoubleAnimation>
		</Storyboard>

		<Storyboard x:Key="RotateTeslaBody">
			<DoubleAnimation
				RepeatBehavior="Forever"
				Storyboard.TargetName="TeslaBody"
				Storyboard.TargetProperty="(UIElement.RenderTransform).(RotateTransform.Angle)"
				To="0.2">
				<DoubleAnimation.EasingFunction>
					<BounceEase
						Bounces="10"
						Bounciness="2"
						EasingMode="EaseInOut" />
				</DoubleAnimation.EasingFunction>
			</DoubleAnimation>
		</Storyboard>
	</Page.Resources>

	<Grid Background="#FFA4A4A4">
		<Grid.RowDefinitions>
			<RowDefinition Height="*" />
			<RowDefinition Height="400" />
		</Grid.RowDefinitions>

		<!--  Main Background  -->
		<Border HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
